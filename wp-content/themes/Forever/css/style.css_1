@media (max-width: 1400px) {
  .banner-content h2 {font-size: 60px; line-height: 66px;}
  .banner-content p {font-size: 32px; line-height: 40px; max-width: 70%;}
}

@media (max-width: 1024px) {
  body {padding-top: 110px;}
  #header #logo {padding: 0 0 6px;}
  .banner-section .owl-carousel, .banner-section .owl-carousel .owl-item .banner-box {height: auto;}
  .banner-content {padding: 50px 0;}
  .banner-content h2 {font-size: 40px; line-height: 46px;}
  .banner-content p {font-size: 24px; line-height: 30px; max-width: 70%;}
  .stages-nav1, .stages-nav2, .stages-nav3, .stages-nav4 {background-size: contain; width: 100%; height: 265px;}
  .sickness-section .sickness-contbox {padding: 80px 60px;}
  .boosting-section .boosting-contbox {padding: 70px;}
  
  .stages-nav1, .stages-nav1:hover {background: url(../images/01-cleanse-icon-mobile.jpg) no-repeat 50% 50%; background-size: contain;}
  .stages-nav2, .stages-nav2:hover {background: url(../images/02-cleanse-icon-mobile.jpg) no-repeat 50% 50%; background-size: contain;}
  .stages-nav3, .stages-nav3:hover {background: url(../images/03-carry-icon-mobile.jpg) no-repeat 50% 50%; background-size: contain;}
  .stages-nav4, .stages-nav4:hover {background: url(../images/04-care-icon-mobile.jpg) no-repeat 50% 50%; background-size: contain;}
  .search-toggle {display: none}
  form.search-box {padding: 0 0 20px; position: relative;}
  form.search-box input[type='text'] {background-color: #f5f5f5!important; border: none; padding: 10px 20px 10px 50px; width: 100%; background: url(../images/search-img.png) no-repeat 15px 50%;}
  form.search-box a {position: absolute; background: url(../images/close-search.png) no-repeat 0 0; border: none; height: 20px; width: 20px; right: 10px; top: 12px; background-size: contain; display: none}
  form.search-box a.active-close {display: block}
  form.search-box button[type='submit'] {display: none}

}

@media (max-width: 999px) {
  .stages-nav1, .stages-nav2, .stages-nav3, .stages-nav4 {height: 350px; background-position: 50%;}
  .sickness-section .sickness-imgbox span {left: -80px; width: 55%;}
  .boosting-section .boosting-imgbox span {right: -80px; width: 50%;}
  .sickness-section .sickness-imgbox, .boosting-section .boosting-imgbox {padding: 0;}
  .sickness-section .sickness-contbox {padding: 78px 60px 78px 30px; margin: 0;}
  .boosting-section .boosting-contbox {padding: 62px 60px 62px 30px; margin: 0;}
  .sickness-section .sickness-contbox h3, .boosting-section .boosting-contbox h3 {font-size: 20px; line-height: 32px;}
  .sickness-section .sickness-contbox p, .boosting-section .boosting-contbox p {font-size: 20px; line-height: 26px; margin: 0}

  .promise-box .promise-imgbox img {border-radius: 100%;}

  .stages-nav1, .stages-nav1:hover, .stages-nav2, .stages-nav2:hover, .stages-nav3, .stages-nav3:hover, .stages-nav4, .stages-nav4:hover {margin-bottom: 40px;}
}

@media (max-width: 767px) {
  .sub-heading ul li ul {left: -85px;}
  #header {padding: 20px 0;}
  #header.scroll-header {padding: 8px 0px}
  #mobile-nav-toggle {margin: 20px 10px 0 0;}
  #header.scroll-header #mobile-nav-toggle {margin: 10px 10px 0 0;}
  .sub-heading {padding: 5px 10px}
  .banner-section .owl-carousel {height: calc(100vh - 108px);}
  .banner-section .owl-carousel .owl-item .banner-box {height: auto;}
  .banner-section .owl-carousel {border-bottom: #eaeaea 1px solid;}
  .banner-section .owl-theme .owl-nav.disabled+.owl-dots {left: 50%; margin-left: -33px;}
  .img-absolute {position: relative; max-width: none; height: 330px;}
  .banner-content {padding: 20px 30px;}
  .banner-content h2 {font-size: 27px; line-height: 36px;}
  .banner-content p {font-size: 20px; line-height: 26px; max-width: none; letter-spacing: 0}
  .stages-section {padding: 20px 0;}
  .sickness-section, .boosting-section {padding: 30px 0 0;}
  .sickness-section .sickness-imgbox, .boosting-section .boosting-imgbox {width: 100%}
  .sickness-section .sickness-imgbox span {left: -20px; width: 35%;}
  .boosting-section .boosting-imgbox span {right: 10px; width: 35%; bottom: -25px;}
  .sickness-section .sickness-contbox {padding: 30px 15px; width: 100%}
  .boosting-section .boosting-contbox {padding: 30px 15px; width: 100%;}

  .desktop-view {display: none;}
  .mobile-view {display: block;}

  .footer .copyrights {padding: 0 0 25px; border: none;}
  .footer .copyrights .container .row .text-right {text-align: center!important; padding: 0 0 20px;}
  .footer .copyrights p {text-align: center; line-height: 24px;}

  .foot-box1 {padding: 0 15px 20px;}
  .foot-box2 {padding: 20px 15px; text-align: center; border-right: #e6e9eb 1px solid;}
  .foot-box3 {padding: 20px 15px; text-align: center}
  .foot-box4 {padding: 20px 15px; text-align: center; border-top: #e6e9eb 1px solid; border-right: #e6e9eb 1px solid;}
  .foot-box5 {padding: 20px 15px; text-align: center; border-top: #e6e9eb 1px solid;}

  .footer .community-menu ul li {display: inline-block; float: none; margin: 0 10px 0 0;}
  .footer .community-menu ul li a {color: #000}
}

@media (max-width: 370px) {
  .img-absolute {position: relative; max-width: 100%; height: auto;}
}