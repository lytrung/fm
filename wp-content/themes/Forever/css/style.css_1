/* .follow-link span {position: absolute; left: 0; top: 0; width: 100%; height: 100%; background: rgba(0, 0, 0, 0.4);} */

/*---------------*/
/***** Chico *****/
/*---------------*/

figure.follow-link figcaption {position: absolute; top: 50%; left: 0; width: 100%; height: 60px; margin-top: -30px;}
figure.follow-link img {-webkit-transition: opacity 0.35s, -webkit-transform 0.35s; transition: opacity 0.35s, transform 0.35s; -webkit-transform: scale(1.12); transform: scale(1.12);}
figure.follow-link:hover img {opacity: 0.6; -webkit-transform: scale(1); transform: scale(1);}
/* figure.follow-link figcaption {padding: 3em;} */
figure.follow-link figcaption::before {position: absolute; top: 30px; right: 30px; bottom: 30px; left: 30px; content: ''; -webkit-transform: scale(1.1); transform: scale(1.1); display: none;}
figure.follow-link figcaption::before, figure.follow-link p {opacity: 0; -webkit-transition: opacity 0.35s, -webkit-transform 0.35s; transition: opacity 0.35s, transform 0.35s;}
figure.follow-link p {margin: 0 auto; text-align: center; -webkit-transform: scale(1.5); transform: scale(1.5); font-size: 36px; color: #fff}
figure.follow-link:hover figcaption::before, figure.follow-link:hover p {opacity: 1; -webkit-transform: scale(1); transform: scale(1);}


