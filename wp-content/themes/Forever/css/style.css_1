/* Back to top button */
.back-to-top {
  position: fixed;
  display: none;
  background: #71c55d;
  line-height: 1;
  color: #fff;
  padding: 7px 12px 13px 12px;
  font-size: 16px;
  border-radius: 2px;
  right: 15px;
  bottom: 15px;
  transition: background 0.5s;
}

.back-to-top:focus {
  background: #71c55d;
  color: #fff;
  outline: none;
}

.back-to-top:hover {
  background: #55b03f;
  color: #fff;
}

/*--------------------------------------------------------------
# Header
--------------------------------------------------------------*/
#header {
  background: #fff;
  position: fixed;
  left: 0;
  /* top: 0; */
  right: 0;
  top: 30px;
  z-index: 3;
  transition: all .3s, -webkit-all.3s;
  padding: 20px 0;
  border-top: #e6e9eb 1px solid;
  border-bottom: #e6e9eb 1px solid;
}

#header #logo {
  padding: 6px 0 0 0;
}

#header #logo h1 {
  color: #505050;
  font-family: "Open Sans Condensed", sans-serif;
  font-size: 26px;
  font-weight: 500;
  letter-spacing: 1px;
  margin: 0;
}

#header #logo h1 a {
  color: #555;
  line-height: 1;
  display: inline-block;
  text-decoration: none;
}

#header #logo h1 a span {
  color: #71c55d;
}

#header #logo img {
  padding: 0;
  margin: 0;
}

#header.scroll-header {
  background: #fff;
  box-shadow: 0 3px 7px rgba(0, 0, 0, 0.1);
  position: fixed;
  padding: 8px 0;
  top: 0; transform: translateY(0);
}

/* .header-container {margin-left: -15px; margin-right: -15px;} */

.sub-heading {text-transform: uppercase; font-size: 12px; padding: 5px 0; vertical-align: top; color: #f3f3f3}
.sub-heading ul {list-style-type: none; float: right;}
.sub-heading ul li {font-weight: 400; margin: 0 5px; text-transform: uppercase; font-size: 12px; padding: 0; line-height: 0; display: inline-block; float: none; transition: none}
.sub-heading ul li a {color: #000; letter-spacing: 0px; font-size: 12px; font-weight: 600!important}
.sub-heading ul li:nth-child(3) {transition-delay: 0s; margin-right: 0}
.sub-heading ul li ul {width: 120px; background: #fff; box-shadow: 1px 2px 3px #ccc; overflow: hidden; padding: 0; top: 24px; left: -80px;}
.sub-heading ul li ul li {width: 100%; display: block; text-align: left; line-height: 12px; padding: 6px 10px; margin: 0; min-width: auto;}
.sub-heading ul li ul li a {font-weight: 400; text-transform: uppercase; font-size: 12px;}
.sub-heading ul li ul li:hover {background: #f7f9fa;} 
.sub-heading ul li ul li a:hover {color: #000!important}
.sub-header {transition: transform .3s, -webkit-transform .3s; transform: translateZ(0); will-change: transform; height: 30px; position: fixed; width: 100%; top: 0; z-index: 9; background: #fff;}
.sub-header.scroll-sunHeader {transform: translateY(-100%);}

/* #header {overflow: hidden;} */
#header .main-menu li {opacity: 1; transition: opacity 0.2s 0.1s, -webkit-transform 0.2s; transition: transform 0.2s, opacity 0.2s 0.1s; transition: transform 0.2s, opacity 0.2s 0.1s, -webkit-transform 0.2s;}
#header .main-menu li:nth-child(1) {transition-delay: 0.2s;}
#header .main-menu li:nth-child(2) {transition-delay: 0.3s;}
#header .main-menu li:nth-child(3) {transition-delay: 0.4s;}
#header .main-menu li:nth-child(4) {transition-delay: 0.5s;}
#header .main-menu li:nth-child(5) {transition-delay: 0.6s;}
#header .main-menu li:nth-child(6) {transition-delay: 0.7s;} 
#header .main-menu li:hover {transition: all 0s!important}
#header .main-menu li ul {transition-delay: 0s!important; display: none}
#header .main-menu li ul li {transition-delay: 0s!important;}
#header .search-box {position: absolute; left: 0; top: 0; width: 85%; height: 100%; padding-left: 2em; -webkit-transform: translateX(20%); transform: translateX(20%); opacity: 0; transition: all 0.5s 0.4s; background: #ffff; z-index: -1;}
#header .search-box .search-input {border: 0; width: 90%; height: 100%; background-color: transparent; border: #e5e5e5 1px solid; padding: 0 20px; background: #fff; float: left;}
#header .search-box .search-toggle {width: 14px; height: 14px; padding: 0; position: absolute; left: 5px; top: 50%; -webkit-transform: translateY(-50%); transform: translateY(-50%);}
#header.show .main-menu li {-webkit-transform: scale(0.8); transform: scale(0.8); opacity: 0;}
#header.show .search-box {width: 100%; -webkit-transform: translateX(0); transform: translateX(0); opacity: 1; z-index: 1;}
#header .search-box button {background: url(../images/search-btn.jpg) no-repeat 0 0; width: 46px; height: 46px; display: inline-block; cursor: pointer; border: none; background-size: contain}

.search-toggle {position: relative; display: block; height: 20px; width: 20px; float: right; top: -32px; right: -50px; margin-bottom: -32px;}
.search-toggle::before, .search-toggle::after {
  content: "";
  position: absolute;
  display: block;
  transition: all 0.1s;
}
.search-toggle::before {
  border: 2px solid #000;
  border-radius: 50%;
  width: 100%;
  height: 100%;
  left: -2px;
  top: -2px;
}
.search-toggle::after {
  height: 2px;
  width: 7px;
  background: #000;
  top: 17px;
  left: 14px;
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
}
.search-toggle.active::before {
  width: 0;
  border-width: 1px;
  border-radius: 0;
  -webkit-transform: rotate(45deg);
          transform: rotate(45deg);
  top: -1px;
  left: 4px;
}
.search-toggle.active::after {width: 20px; left: -4px; top: 8px;}

.search-input:focus {
  outline: none;
}

/*--------------------------------------------------------------
# Banner Section
--------------------------------------------------------------*/
.banner-section .owl-carousel {height: calc(100vh - 108px); overflow: hidden;}
.banner-section .owl-carousel .owl-item .banner-box {position: relative; height: 100vh;}
.display-table {width: 100%; height: 100%; display: table;}
.table-cell {display: table-cell; vertical-align: middle;}
.banner-content h2 {font-size: 80px; line-height: 90px; font-weight: 900; color: #000;}
.banner-content p {font: 400 46px/52px 'Open Sans Condensed', sans-serif; color: #000; letter-spacing: -1px;}
.banner-content p a {letter-spacing: -1px;}
.banner-content p a:hover {border: none; text-decoration: none}

.banner-section .owl-theme .owl-nav.disabled+.owl-dots {margin-top: 10px; position: absolute; z-index: 9999; bottom: 15px; left: 48%;}
.banner-section .owl-theme .owl-dots .owl-dot span {width: 12px; height: 12px; border: #b4bbbf 1px solid; background: none; margin: 5px;}
.banner-section .owl-theme .owl-dots .owl-dot.active span {background: #b4bbbf;}

.lrn-mor1 {color: #6e9eba; border-bottom: #6e9eba 1px solid;}
.lrn-mor2 {color: #82b7ae; border-bottom: #82b7ae 1px solid;}
.lrn-mor3 {color: #f3bdc6; border-bottom: #f3bdc6 1px solid;}


/*--------------------------------------------------------------
# Navigation Menu
--------------------------------------------------------------*/
/* Nav Menu Essentials */
.nav-menu, .nav-menu * {
  margin: 0;
  padding: 0;
  list-style: none;
}

.nav-menu ul {
  position: absolute;
  display: none;
  top: 100%;
  left: -20px;
  z-index: 99;
}

.nav-menu li {
  position: relative;
  white-space: nowrap;
}

.nav-menu > li {
  float: left;
}

.nav-menu li:hover > ul,
.nav-menu li.sfHover > ul {
  display: block;
}

.nav-menu ul ul {
  top: 0;
  left: 100%;
}

.nav-menu ul li {
  min-width: 180px;
}

/* Nav Menu Arrows */
.sf-arrows .sf-with-ul, .nav-menu ul li a.sf-with-ul {
  padding-right: 22px;
}

.sf-arrows .sf-with-ul:after, .nav-menu ul li a.sf-with-ul:after {
  content: "\f078";
  position: absolute;
  right: 6px;
  margin-top: 1px;
  font-family: FontAwesome;
  font-style: normal;
  font-weight: normal;
  font-size: 12px;
}

.sf-arrows ul .sf-with-ul:after, .nav-menu ul li a.sf-with-ul:after {
  content: "\f105";
}

/* Nav Meu Container */
#nav-menu-container {
  float: right;
  margin: 0 50px 0 0;
  position: relative;
}

@media (max-width: 1024px) {
  #nav-menu-container {
    display: none;
  }
}

/* Nav Meu Styling */
.nav-menu a {
  padding: 8px 0;
  text-decoration: none;
  display: inline-block;
  color: #000;
  /* font-family: "Roboto", sans-serif; */
  font-weight: 300;
  font-size: 18px;
  outline: none;
  letter-spacing: 0;
  transition: 0.3s;
}

.nav-menu li:hover > a, .nav-menu > .menu-active > a {
  color: #71c55d;
}

.nav-menu > li {
  margin-left: 45px;
}

.nav-menu ul {
  margin: 0;
  padding: 10px;
  /* box-shadow: 0px 0px 30px rgba(127, 137, 161, 0.25); */
  background: #fff;
}

.nav-menu ul li {
  transition: 0.3s;
}

.nav-menu ul li a {
  padding: 5px 10px;
  color: #000;
  transition: 0.3s;
  display: block;
  font-size: 18px;
  text-transform: none;
}

.nav-menu ul li:hover > a {
  color: #71c55d;
}

.nav-menu ul ul {
  margin: 0;
}

/* Mobile Nav Toggle */
#mobile-nav-toggle {
  position: absolute;
  right: 0;
  top: 0;
  z-index: 999;
  margin: 20px 30px 0 0;
  border: 0;
  background: none;
  font-size: 24px;
  display: none;
  transition: all 0.4s;
  outline: none;
  cursor: pointer;
}

#header.scroll-header #mobile-nav-toggle {
  margin: 10px 30px 0 0;
}

#mobile-nav-toggle i {
  color: #000;
  font-size: 24px;
}

@media (max-width: 1024px) {
  #mobile-nav-toggle {
    display: inline;
  }
}

/* Mobile Nav Styling */
#mobile-nav {
  position: fixed;
  top: 112px;
  padding: 20px;
  bottom: 0;
  z-index: 998;
  background: #fff;
  left: -100%;
  width: 100%;
  overflow-y: auto;
  transition: 0.4s;
}

#mobile-nav ul {
  padding: 0;
  margin: 0;
  list-style: none;
  border-top: #f2f2f2 1px solid;
}

#mobile-nav ul li {
  position: relative;
  border-bottom: #f2f2f2 1px solid;
}

#mobile-nav ul li a {
  color: #000;
  font-size: 15px;
  overflow: hidden;
  padding: 6px 22px 6px 15px;
  position: relative;
  text-decoration: none;
  width: 100%;
  display: block;
  outline: none;
  letter-spacing: 0;
}

#mobile-nav ul li a:hover {
  color: #000;
}

#mobile-nav ul li li {
  padding-left: 30px;
  border: none;
}

#mobile-nav ul .menu-item i {
  position: absolute;
  right: 0;
  z-index: 99;
  padding: 15px;
  cursor: pointer;
  color: #000;
}

#mobile-nav ul .menu-item i.fa-chevron-up {
  color: #000;
}

#mobile-nav ul .menu-item-active {
  color: #000;
}

#mobile-body-overly {
  width: 100%;
  height: 100%;
  z-index: 997;
  top: 0;
  left: 0;
  position: fixed;
  /* background: rgba(52, 59, 64, 0.9); */
  display: none;
}

#mobile-nav.scroll-nav {
  top: 58px!important;
}

/* Mobile Nav body classes */
body.mobile-nav-active {
  overflow: hidden;
}

body.mobile-nav-active #mobile-nav {
  left: 0;
}

body.mobile-nav-active #mobile-nav-toggle i {
  color: #000;
}

.scroll-header + #mobile-nav {
  top: 0;
}

#mobile-nav ul li ul {
  padding: 10px 0;
}

#mobile-nav ul li li a {
  line-height: 15px;
  padding: 12px 22px 12px 15px;
}

